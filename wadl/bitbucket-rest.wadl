                                    mediaType="application/json"><ns2:doc><ns3:p
                            xmlns:ns3="http://www.w3.org/1999/xhtml"><ns3:h6>Example</ns3:h6><ns3:pre><ns3:code>{"errors":[{"context":null,"message":"A detailed error message.","exceptionName":null}]}</ns3:code></ns3:pre></ns3:p></ns2:doc><ns2:doc><![CDATA[The currently authenticated user has insufficient permissions to view the
 project.]]></ns2:doc></ns2:representation><ns2:representation
                            element="errors" status="404"
                                    mediaType="application/json"><ns2:doc><ns3:p
                    xmlns:ns3="http://www.w3.org/1999/xhtml"><ns3:h6>Example</ns3:h6><ns3:pre><ns3:code>{"errors":[{"context":null,"message":"A detailed error message.","exceptionName":null}]}</ns3:code></ns3:pre></ns3:p></ns2:doc><ns2:doc><![CDATA[The specified project does not exist.]]></ns2:doc></ns2:representation></ns2:response></ns2:method><ns2:method
                    id="uploadAvatar"
                            name="POST"><ns2:doc><![CDATA[Update the avatar for the project matching the supplied <strong>projectKey</strong>.
 <p>
 This resource accepts POST multipart form data, containing a single image in a form-field named 'avatar'.
 <p>
 There are configurable server limits on both the dimensions (1024x1024 pixels by default) and uploaded file size
 (1MB by default). Several different image formats are supported, but <strong>PNG</strong> and
 <strong>JPEG</strong> are preferred due to the file size limit.
 <p>
 This resource has Cross-Site Request Forgery (XSRF) protection. To allow the request to
 pass the XSRF check the caller needs to send an <code>X-Atlassian-Token</code> HTTP header with the
 value <code>no-check</code>.
 <p>
 An example <a href="http://curl.haxx.se/">curl</a> request to upload an image name 'avatar.png' would be:
 <pre>
 curl -X POST -u username:password -H "X-Atlassian-Token: no-check" http://example.com/rest/api/1.0/projects/STASH/avatar.png -F avatar=@avatar.png
 </pre>
 <p>
 The authenticated user must have <strong>PROJECT_ADMIN</strong> permission for the specified project to call this
 resource.]]></ns2:doc><ns2:response><ns2:representation
                            status="201"><ns2:doc><![CDATA[The avatar was uploaded successfully.]]></ns2:doc></ns2:representation><ns2:representation
                            element="errors" status="401"
                                    mediaType="application/json"><ns2:doc><ns3:p
                            xmlns:ns3="http://www.w3.org/1999/xhtml"><ns3:h6>Example</ns3:h6><ns3:pre><ns3:code>{"errors":[{"context":null,"message":"A detailed error message.","exceptionName":null}]}</ns3:code></ns3:pre></ns3:p></ns2:doc><ns2:doc><![CDATA[The currently authenticated user has insufficient permissions to update the
 project.]]></ns2:doc></ns2:representation><ns2:representation
                            element="errors" status="404"
                                    mediaType="application/json"><ns2:doc><ns3:p
            xmlns:ns3="http://www.w3.org/1999/xhtml"><ns3:h6>Example</ns3:h6><ns3:pre><ns3:code>{"errors":[{"context":null,"message":"A detailed error message.","exceptionName":null}]}</ns3:code></ns3:pre></ns3:p></ns2:doc><ns2:doc><![CDATA[The specified project does not exist.]]></ns2:doc></ns2:representation></ns2:response></ns2:method></ns2:resource></ns2:resource><ns2:resource
                path="api/1.0/projects/{projectKey}/repos/{repositorySlug}/pull-requests/{pullRequestId}/tasks"><ns2:method
                id="getPullRequestTasks"
                        name="GET"><ns2:doc><![CDATA[Retrieve the tasks associated with a pull request.]]></ns2:doc><ns2:response><ns2:representation
                        element="task" status="200"
                                mediaType="application/json"><ns2:doc><ns3:p
                        xmlns:ns3="http://www.w3.org/1999/xhtml"><ns3:h6>Example</ns3:h6><ns3:pre><ns3:code>{"size":2,"limit":25,"isLastPage":true,"values":[{"properties":{"diffAnchorPath":"path/to/file.txt"},"anchor":{"properties":{"key":"value"},"id":1,"version":1,"text":"An insightful comment.","author":{"name":"jcitizen","emailAddress":"jane@example.com","id":101,"displayName":"Jane Citizen","active":true,"slug":"jcitizen","type":"NORMAL"},"createdDate":1459818992944,"updatedDate":1459818992944,"comments":[],"tasks":[],"permittedOperations":{"editable":true,"deletable":true}},"author":{"name":"jcitizen","emailAddress":"jane@example.com","id":101,"displayName":"Jane Citizen","active":true,"slug":"jcitizen","type":"NORMAL"},"createdDate":1459818994173,"id":98,"permittedOperations":{"deletable":true,"editable":true,"transitionable":true},"text":"Fix typos on the constructor's arguments","state":"OPEN"},{"properties":{"diffAnchorPath":"path/to/file.txt"},"anchor":{"properties":{"key":"value"},"id":5,"version":1,"text":"This class is deprecated.  Can you please use the replacement?","author":{"name":"jcitizen","emailAddress":"jane@example.com","id":101,"displayName":"Jane Citizen","active":true,"slug":"jcitizen","type":"NORMAL"},"createdDate":1459818992944,"updatedDate":1459818992944,"comments":[],"tasks":[],"permittedOperations":{"editable":true,"deletable":true}},"author":{"name":"jcitizen","emailAddress":"jane@example.com","id":101,"displayName":"Jane Citizen","active":true,"slug":"jcitizen","type":"NORMAL"},"createdDate":1459818994173,"id":99,"permittedOperations":{"deletable":false,"editable":false,"transitionable":true},"text":"Remove usage of deprecated class","state":"RESOLVED"}],"start":0}</ns3:code></ns3:pre></ns3:p></ns2:doc><ns2:doc><![CDATA[A page of tasks associated with the pull request.]]></ns2:doc></ns2:representation><ns2:representation
                        element="errors" status="401"
                                mediaType="application/json"><ns2:doc><ns3:p
                        xmlns:ns3="http://www.w3.org/1999/xhtml"><ns3:h6>Example</ns3:h6><ns3:pre><ns3:code>{"errors":[{"context":null,"message":"A detailed error message.","exceptionName":null}]}</ns3:code></ns3:pre></ns3:p></ns2:doc><ns2:doc><![CDATA[If the current user cannot access the pull request]]></ns2:doc></ns2:representation><ns2:representation
                        element="errors" status="404"
                                mediaType="application/json"><ns2:doc><ns3:p
                xmlns:ns3="http://www.w3.org/1999/xhtml"><ns3:h6>Example</ns3:h6><ns3:pre><ns3:code>{"errors":[{"context":null,"message":"A detailed error message.","exceptionName":null}]}</ns3:code></ns3:pre></ns3:p></ns2:doc><ns2:doc><![CDATA[If the pull request does not exist]]></ns2:doc></ns2:representation></ns2:response></ns2:method><ns2:resource
                    path="count"><ns2:method id="countPullRequestTasks"
                            name="GET"><ns2:doc><![CDATA[Retrieve the total number of {@link TaskState#OPEN open} and
 {@link TaskState#RESOLVED resolved} tasks associated with a pull request.]]></ns2:doc><ns2:response><ns2:representation
                            element="taskCount" status="200"
                                    mediaType="application/json"><ns2:doc><ns3:p
                            xmlns:ns3="http://www.w3.org/1999/xhtml"><ns3:h6>Example</ns3:h6><ns3:pre><ns3:code>{"open":5,"resolved":2}</ns3:code></ns3:pre></ns3:p></ns2:doc><ns2:doc><![CDATA[Total number of {@link TaskState#OPEN open} and
 {@link TaskState#RESOLVED resolved} tasks associated with the pull request.]]></ns2:doc></ns2:representation><ns2:representation
                            element="errors" status="401"
                                    mediaType="application/json"><ns2:doc><ns3:p
                            xmlns:ns3="http://www.w3.org/1999/xhtml"><ns3:h6>Example</ns3:h6><ns3:pre><ns3:code>{"errors":[{"context":null,"message":"A detailed error message.","exceptionName":null}]}</ns3:code></ns3:pre></ns3:p></ns2:doc><ns2:doc><![CDATA[If the current user cannot access the pull request]]></ns2:doc></ns2:representation><ns2:representation
                            element="errors" status="404"
                                    mediaType="application/json"><ns2:doc><ns3:p
            xmlns:ns3="http://www.w3.org/1999/xhtml"><ns3:h6>Example</ns3:h6><ns3:pre><ns3:code>{"errors":[{"context":null,"message":"A detailed error message.","exceptionName":null}]}</ns3:code></ns3:pre></ns3:p></ns2:doc><ns2:doc><![CDATA[If the pull request does not exist]]></ns2:doc></ns2:representation></ns2:response></ns2:method></ns2:resource></ns2:resource><ns2:resource
                path="api/1.0//inbox"><ns2:resource
                    path="pull-requests/count"><ns2:method
                    id="getPullRequestCount"
                            name="GET"><ns2:response><ns2:representation
                mediaType="application/json; charset=UTF-8"/></ns2:response></ns2:method></ns2:resource><ns2:resource
                    path="pull-requests"><ns2:method
                    id="getPullRequests"
                            name="GET"><ns2:request><ns2:param
                            name="start" style="query" type="xs:int"
                            default="0"
                            xmlns:xs="http://www.w3.org/2001/XMLSchema"/><ns2:param
                            name="limit" style="query" type="xs:int"
                            default="25"
                            xmlns:xs="http://www.w3.org/2001/XMLSchema"/><ns2:param
                            name="role" style="query" type="xs:string"
                            default="reviewer"
                            xmlns:xs="http://www.w3.org/2001/XMLSchema"/></ns2:request><ns2:response><ns2:representation
            mediaType="application/json; charset=UTF-8"/></ns2:response></ns2:method></ns2:resource></ns2:resource><ns2:resource
                path="api/1.0/admin/license"><ns2:doc><![CDATA[A REST endpoint for retrieving or updating the license.]]></ns2:doc><ns2:method
                id="get"
                        name="GET"><ns2:doc><![CDATA[Retrieves details about the current license, as well as the current status of the system with regards to the
 installed license. The status includes the current number of users applied toward the license limit, as well
 as any status messages about the license (warnings about expiry or user counts exceeding license limits).
 <p>
 The authenticated user must have <b>ADMIN</b> permission. Unauthenticated users, and non-administrators, are
 not permitted to access license details.]]></ns2:doc><ns2:response><ns2:representation
                        element="license" status="200"
                                mediaType="application/json"><ns2:doc><ns3:p
                        xmlns:ns3="http://www.w3.org/1999/xhtml"><ns3:h6>Example</ns3:h6><ns3:pre><ns3:code>{"creationDate":1331038800000,"purchaseDate":1331038800000,"expiryDate":1372493732817,"numberOfDaysBeforeExpiry":0,"maintenanceExpiryDate":1372493732817,"numberOfDaysBeforeMaintenanceExpiry":0,"gracePeriodEndDate":1372493732817,"numberOfDaysBeforeGracePeriodExpiry":0,"maximumNumberOfUsers":12,"unlimitedNumberOfUsers":false,"serverId":"&lt;server ID embedded in license&gt;","supportEntitlementNumber":"&lt;support entitlement number embedded in license&gt;","license":"&lt;encoded license text&gt;","status":{"serverId":"&lt;actual server ID&gt;","currentNumberOfUsers":2}}</ns3:code></ns3:pre></ns3:p></ns2:doc><ns2:doc><![CDATA[The currently-installed license.]]></ns2:doc></ns2:representation><ns2:representation
                        element="errors" status="401"
                                mediaType="application/json"><ns2:doc><ns3:p
                        xmlns:ns3="http://www.w3.org/1999/xhtml"><ns3:h6>Example</ns3:h6><ns3:pre><ns3:code>{"errors":[{"context":null,"message":"A detailed error message.","exceptionName":null}]}</ns3:code></ns3:pre></ns3:p></ns2:doc><ns2:doc><![CDATA[The currently authenticated user has insufficient permissions to view the
 license, or the request is anonymous.]]></ns2:doc></ns2:representation><ns2:representation
                        element="errors" status="404"
                                mediaType="application/json"><ns2:doc><ns3:p
                xmlns:ns3="http://www.w3.org/1999/xhtml"><ns3:h6>Example</ns3:h6><ns3:pre><ns3:code>{"errors":[{"context":null,"message":"A detailed error message.","exceptionName":null}]}</ns3:code></ns3:pre></ns3:p></ns2:doc><ns2:doc><![CDATA[No license has been installed.]]></ns2:doc></ns2:representation></ns2:response></ns2:method><ns2:method
                id="update"
                        name="POST"><ns2:doc><![CDATA[Decodes the provided encoded license and sets it as the active license. If no license was provided, a 400 is
 returned. If the license cannot be decoded, or cannot be applied, a 409 is returned. Some possible reasons a
 license may not be applied include:
 <ul>
     <li>It is for a different product</li>
     <li>It is already expired</li>
 </ul>
 Otherwise, if the license is updated successfully, details for the new license are returned with a 200 response.
 <p>
 <b>Warning</b>: It is possible to downgrade the license during update, applying a license with a lower number
 of permitted users. If the number of currently-licensed users exceeds the limits of the new license, pushing
 will be disabled until the licensed user count is brought into compliance with the new license.
 <p>
 The authenticated user must have <b>SYS_ADMIN</b> permission. <b>ADMIN</b> users may <i>view</i> the current
 license details, but they may not <i>update</i> the license.]]></ns2:doc><ns2:request><ns2:representation
                                mediaType="application/json"><ns2:doc><ns3:p
                        xmlns:ns3="http://www.w3.org/1999/xhtml"><ns3:h6>Example</ns3:h6><ns3:pre><ns3:code>{"license":"&lt;encoded license text&gt;"}</ns3:code></ns3:pre></ns3:p></ns2:doc></ns2:representation></ns2:request><ns2:response><ns2:representation
                        element="license" status="200"
                                mediaType="application/json"><ns2:doc><ns3:p
                        xmlns:ns3="http://www.w3.org/1999/xhtml"><ns3:h6>Example</ns3:h6><ns3:pre><ns3:code>{"creationDate":1331038800000,"purchaseDate":1331038800000,"expiryDate":1372493732817,"numberOfDaysBeforeExpiry":0,"maintenanceExpiryDate":1372493732817,"numberOfDaysBeforeMaintenanceExpiry":0,"gracePeriodEndDate":1372493732817,"numberOfDaysBeforeGracePeriodExpiry":0,"maximumNumberOfUsers":12,"unlimitedNumberOfUsers":false,"serverId":"&lt;server ID embedded in license&gt;","supportEntitlementNumber":"&lt;support entitlement number embedded in license&gt;","license":"&lt;encoded license text&gt;","status":{"serverId":"&lt;actual server ID&gt;","currentNumberOfUsers":2}}</ns3:code></ns3:pre></ns3:p></ns2:doc><ns2:doc><![CDATA[The newly-installed license.]]></ns2:doc></ns2:representation><ns2:representation
                        element="errors" status="400"
                                mediaType="application/json"><ns2:doc><ns3:p
                        xmlns:ns3="http://www.w3.org/1999/xhtml"><ns3:h6>Example</ns3:h6><ns3:pre><ns3:code>{"errors":[{"context":"field_a","message":"A detailed validation error message for field_a.","exceptionName":null},{"context":null,"message":"A detailed error message.","exceptionName":null}]}</ns3:code></ns3:pre></ns3:p></ns2:doc><ns2:doc><![CDATA[No encoded license was provided in the JSON body for the POST.]]></ns2:doc></ns2:representation><ns2:representation
                        element="errors" status="401"
                                mediaType="application/json"><ns2:doc><ns3:p
                        xmlns:ns3="http://www.w3.org/1999/xhtml"><ns3:h6>Example</ns3:h6><ns3:pre><ns3:code>{"errors":[{"context":null,"message":"A detailed error message.","exceptionName":null}]}</ns3:code></ns3:pre></ns3:p></ns2:doc><ns2:doc><![CDATA[The currently authenticated user has insufficient permissions to update the
                                  license.]]></ns2:doc></ns2:representation><ns2:representation
                        element="errors" status="409"
                                mediaType="application/json"><ns2:doc><ns3:p
            xmlns:ns3="http://www.w3.org/1999/xhtml"><ns3:h6>Example</ns3:h6><ns3:pre><ns3:code>{"errors":[{"context":null,"message":"A detailed error message.","exceptionName":null}]}</ns3:code></ns3:pre></ns3:p></ns2:doc><ns2:doc><![CDATA[The encoded license could not be decoded, or it is not valid for use on this
                                  server. For example, it may be for a different product, or it may have already
                                  expired.]]></ns2:doc></ns2:representation></ns2:response></ns2:method></ns2:resource><ns2:resource
                path="api/1.0/profile/recent/repos"><ns2:method
                id="getRepositoriesRecentlyAccessed"
                        name="GET"><ns2:doc><![CDATA[Retrieve a page of recently accessed repositories for the currently authenticated user.
 <p>
 Repositories are ordered from most recently to least recently accessed.
 <p>
 Only authenticated users may call this resource.]]></ns2:doc><ns2:request><ns2:param
                        name="permission" style="query" type="xs:string"
                        xmlns:xs="http://www.w3.org/2001/XMLSchema"><ns2:doc><![CDATA[(optional) if specified, it must be a valid repository permission level name and will limit
                   the resulting repository list to ones that the requesting user has the specified permission
                   level to. If not specified, the default <code>REPO_READ</code> permission level will be assumed.]]></ns2:doc></ns2:param></ns2:request><ns2:response><ns2:representation
                        element="page" status="200"
                                mediaType="application/json"><ns2:doc><ns3:p
                        xmlns:ns3="http://www.w3.org/1999/xhtml"><ns3:h6>Example</ns3:h6><ns3:pre><ns3:code>{"size":1,"limit":25,"isLastPage":true,"values":[{"slug":"my-repo","id":1,"name":"My repo","scmId":"git","state":"AVAILABLE","statusMessage":"Available","forkable":true,"project":{"key":"PRJ","id":1,"name":"My Cool Project","description":"The description for my cool project.","public":true,"type":"NORMAL","links":{"self":[{"href":"http://link/to/project"}]}},"public":true,"links":{"clone":[{"href":"ssh://git@&lt;baseURL&gt;/PRJ/my-repo.git","name":"ssh"},{"href":"https://&lt;baseURL&gt;/scm/PRJ/my-repo.git","name":"http"}],"self":[{"href":"http://link/to/repository"}]}}],"start":0}</ns3:code></ns3:pre></ns3:p></ns2:doc><ns2:doc><![CDATA[A page of recently accessed repositories.]]></ns2:doc></ns2:representation><ns2:representation
                        element="errors" status="400"
                                mediaType="application/json"><ns2:doc><ns3:p
                        xmlns:ns3="http://www.w3.org/1999/xhtml"><ns3:h6>Example</ns3:h6><ns3:pre><ns3:code>{"errors":[{"context":null,"message":"A detailed error message.","exceptionName":null}]}</ns3:code></ns3:pre></ns3:p></ns2:doc><ns2:doc><![CDATA[The permission level is unknown or not related to repository.]]></ns2:doc></ns2:representation><ns2:representation
                        element="errors" status="401"
                                mediaType="application/json"><ns2:doc><ns3:p
            xmlns:ns3="http://www.w3.org/1999/xhtml"><ns3:h6>Example</ns3:h6><ns3:pre><ns3:code>{"errors":[{"context":null,"message":"A detailed error message.","exceptionName":null}]}</ns3:code></ns3:pre></ns3:p></ns2:doc><ns2:doc><![CDATA[The request is unauthenticated.]]></ns2:doc></ns2:representation></ns2:response></ns2:method></ns2:resource><ns2:resource
                path="api/1.0/projects/{projectKey}/permissions"><ns2:resource
                    path="users/none"><ns2:method
                    id="getUsersWithoutPermission"
                            name="GET"><ns2:doc><![CDATA[Retrieve a page of <i>licensed</i> users that have no granted permissions for the specified project.
 <p>
 The authenticated user must have <strong>PROJECT_ADMIN</strong> permission for the specified project or a higher
 global permission to call this resource.]]></ns2:doc><ns2:request><ns2:param
                            name="filter" style="query" type="xs:string"
                            xmlns:xs="http://www.w3.org/2001/XMLSchema"><ns2:doc><![CDATA[if specified only group names containing the supplied string will be returned]]></ns2:doc></ns2:param></ns2:request><ns2:response><ns2:representation
                            element="page" status="200"
                                    mediaType="application/json"><ns2:doc><ns3:p
                            xmlns:ns3="http://www.w3.org/1999/xhtml"><ns3:h6>Example</ns3:h6><ns3:pre><ns3:code>{"size":1,"limit":25,"isLastPage":true,"values":[{"name":"group-a","deletable":true}],"start":0}</ns3:code></ns3:pre></ns3:p></ns2:doc><ns2:doc><![CDATA[A page of users that have not been granted any permissions for the specified
                                  project.]]></ns2:doc></ns2:representation><ns2:representation
                            element="errors" status="401"
                                    mediaType="application/json"><ns2:doc><ns3:p
                            xmlns:ns3="http://www.w3.org/1999/xhtml"><ns3:h6>Example</ns3:h6><ns3:pre><ns3:code>{"errors":[{"context":null,"message":"A detailed error message.","exceptionName":null}]}</ns3:code></ns3:pre></ns3:p></ns2:doc><ns2:doc><![CDATA[The currently authenticated user is not a project administrator for the
                                  specified project.]]></ns2:doc></ns2:representation><ns2:representation
                            element="errors" status="404"
                                    mediaType="application/json"><ns2:doc><ns3:p
                xmlns:ns3="http://www.w3.org/1999/xhtml"><ns3:h6>Example</ns3:h6><ns3:pre><ns3:code>{"errors":[{"context":null,"message":"A detailed error message.","exceptionName":null}]}</ns3:code></ns3:pre></ns3:p></ns2:doc><ns2:doc><![CDATA[The specified project does not exist.]]></ns2:doc></ns2:representation></ns2:response></ns2:method></ns2:resource><ns2:resource
                    path="{permission}/all"><ns2:param name="permission"
                    style="template" type="xs:string"
                    xmlns:xs="http://www.w3.org/2001/XMLSchema"><ns2:doc><![CDATA[the permission to grant]]></ns2:doc></ns2:param><ns2:method
                    id="hasAllUserPermission"
                            name="GET"><ns2:doc><![CDATA[Check whether the specified permission is the default permission (granted to all users) for a project. Available
 project permissions are:
 <ul>
     <li>PROJECT_READ</li>
     <li>PROJECT_WRITE</li>
     <li>PROJECT_ADMIN</li>
 </ul>
 <p>
 The authenticated user must have <strong>PROJECT_ADMIN</strong> permission for the specified project or a higher
 global permission to call this resource.]]></ns2:doc><ns2:response><ns2:representation
                            element="permitted" status="200"
                                    mediaType="application/json"><ns2:doc><ns3:p
                            xmlns:ns3="http://www.w3.org/1999/xhtml"><ns3:h6>Example</ns3:h6><ns3:pre><ns3:code>{"permitted":true}</ns3:code></ns3:pre></ns3:p></ns2:doc><ns2:doc><![CDATA[A simple entity indicating whether the specified permission is the default
                                  permission for this project.]]></ns2:doc></ns2:representation><ns2:representation
