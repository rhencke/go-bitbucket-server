                path="api/1.0/markup"><ns2:resource
                    path="preview"><ns2:method id="preview"
                            name="POST"><ns2:doc><![CDATA[Preview the generated html for given markdown contents.
 <p>
 Only authenticated users may call this resource.]]></ns2:doc><ns2:request><ns2:param
                            name="urlMode" style="query"
                            type="xs:string"
                            xmlns:xs="http://www.w3.org/2001/XMLSchema"/><ns2:param
                            name="hardwrap" style="query"
                            type="xs:boolean"
                            xmlns:xs="http://www.w3.org/2001/XMLSchema"/><ns2:param
                            name="htmlEscape" style="query"
                            type="xs:boolean"
                            xmlns:xs="http://www.w3.org/2001/XMLSchema"/><ns2:representation
                                    mediaType="*/*"><ns2:doc><ns3:p
                            xmlns:ns3="http://www.w3.org/1999/xhtml"><ns3:h6>Example</ns3:h6><ns3:pre><ns3:code># Hello World!</ns3:code></ns3:pre></ns3:p></ns2:doc></ns2:representation></ns2:request><ns2:response><ns2:representation
