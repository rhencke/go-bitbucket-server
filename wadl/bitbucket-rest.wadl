                            name="GET"><ns2:doc><![CDATA[Retrieve the project matching the supplied <strong>projectKey</strong>.
 <p>
 The authenticated user must have <strong>PROJECT_VIEW</strong> permission for the specified project to call this
 resource.]]></ns2:doc><ns2:response><ns2:representation
                            element="project" status="200"
                                    mediaType="application/json"><ns2:doc><ns3:p
                            xmlns:ns3="http://www.w3.org/1999/xhtml"><ns3:h6>Example</ns3:h6><ns3:pre><ns3:code>{"key":"PRJ","id":1,"name":"My Cool Project","description":"The description for my cool project.","public":true,"type":"NORMAL","links":{"self":[{"href":"http://link/to/project"}]}}</ns3:code></ns3:pre></ns3:p></ns2:doc><ns2:doc><![CDATA[The project matching the supplied <strong>projectKey</strong>.]]></ns2:doc></ns2:representation><ns2:representation
                            element="errors" status="401"
                                    mediaType="application/json"><ns2:doc><ns3:p
                            xmlns:ns3="http://www.w3.org/1999/xhtml"><ns3:h6>Example</ns3:h6><ns3:pre><ns3:code>{"errors":[{"context":null,"message":"A detailed error message.","exceptionName":null}]}</ns3:code></ns3:pre></ns3:p></ns2:doc><ns2:doc><![CDATA[The currently authenticated user has insufficient permissions to view the
 project.]]></ns2:doc></ns2:representation><ns2:representation
                            element="errors" status="404"
                                    mediaType="application/json"><ns2:doc><ns3:p
