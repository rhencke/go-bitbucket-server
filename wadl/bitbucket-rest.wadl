                path="api/1.0/projects/{projectKey}/repos/{repositorySlug}/participants"><ns2:method
                id="search"
                        name="GET"><ns2:doc><![CDATA[Retrieve a page of participant users for all the pull requests to or from the specified repository.
 <p>
 <p>
 Optionally clients can specify following filters.]]></ns2:doc><ns2:request><ns2:param
                        name="direction" style="query" type="xs:string"
                        default="incoming"
                        xmlns:xs="http://www.w3.org/2001/XMLSchema"><ns2:doc><![CDATA[(optional, defaults to <strong>INCOMING</strong>) the direction relative to the specified
                  repository. Either <strong>INCOMING</strong> or <strong>OUTGOING</strong>.]]></ns2:doc></ns2:param><ns2:param
                        name="filter" style="query" type="xs:string"
                        xmlns:xs="http://www.w3.org/2001/XMLSchema"><ns2:doc><![CDATA[(optional) return only users, whose username, name or email address <i>contain</i>
                  the {@code filter} value]]></ns2:doc></ns2:param><ns2:param
                        name="role" style="query" type="xs:string"
                        xmlns:xs="http://www.w3.org/2001/XMLSchema"><ns2:doc><![CDATA[(optional) The role associated with the pull request participant.
                  This must be one of {@code AUTHOR}, {@code REVIEWER}, or{@code PARTICIPANT}]]></ns2:doc></ns2:param></ns2:request><ns2:response><ns2:representation
                        element="page" status="200"
                                mediaType="application/json"><ns2:doc><ns3:p
                        xmlns:ns3="http://www.w3.org/1999/xhtml"><ns3:h6>Example</ns3:h6><ns3:pre><ns3:code>{"name":"charlie","emailAddress":"charlie@example.com"}</ns3:code></ns3:pre></ns3:p></ns2:doc><ns2:doc><![CDATA[A page of users that match the search criteria.]]></ns2:doc></ns2:representation><ns2:representation
                        element="errors" status="400"
                                mediaType="application/json"><ns2:doc><ns3:p
                        xmlns:ns3="http://www.w3.org/1999/xhtml"><ns3:h6>Example</ns3:h6><ns3:pre><ns3:code>{"errors":[{"context":"field_a","message":"A detailed validation error message for field_a.","exceptionName":null},{"context":null,"message":"A detailed error message.","exceptionName":null}]}</ns3:code></ns3:pre></ns3:p></ns2:doc><ns2:doc><![CDATA[The request was malformed.]]></ns2:doc></ns2:representation><ns2:representation
                        element="errors" status="401"
                                mediaType="application/json"><ns2:doc><ns3:p
                        xmlns:ns3="http://www.w3.org/1999/xhtml"><ns3:h6>Example</ns3:h6><ns3:pre><ns3:code>{"errors":[{"context":null,"message":"A detailed error message.","exceptionName":null}]}</ns3:code></ns3:pre></ns3:p></ns2:doc><ns2:doc><![CDATA[The currently authenticated user has insufficient permissions to view the specified repository.]]></ns2:doc></ns2:representation><ns2:representation
                        element="errors" status="404"
                                mediaType="application/json"><ns2:doc><ns3:p
            xmlns:ns3="http://www.w3.org/1999/xhtml"><ns3:h6>Example</ns3:h6><ns3:pre><ns3:code>{"errors":[{"context":null,"message":"A detailed error message.","exceptionName":null}]}</ns3:code></ns3:pre></ns3:p></ns2:doc><ns2:doc><![CDATA[The specified repository does not exist.]]></ns2:doc></ns2:representation></ns2:response></ns2:method></ns2:resource><ns2:resource
                path="api/1.0/projects/{projectKey}/repos/{repositorySlug}/commits/{commitId}/watch"><ns2:param
                name="commitId" style="template" type="xs:string"
                xmlns:xs="http://www.w3.org/2001/XMLSchema"><ns2:doc><![CDATA[the <i>full {@link Commit#getId() ID}</i> of the commit within the repository]]></ns2:doc></ns2:param><ns2:method
                id="unwatch"
                        name="DELETE"><ns2:doc><![CDATA[Removes the authenticated user as a watcher for the specified commit.
 <p>
 The authenticated user must have <strong>REPO_READ</strong> permission for the repository containing the commit
 to call this resource.]]></ns2:doc><ns2:response><ns2:representation
                        element="errors" status="401"
                                mediaType="application/json"><ns2:doc><ns3:p
                        xmlns:ns3="http://www.w3.org/1999/xhtml"><ns3:h6>Example</ns3:h6><ns3:pre><ns3:code>{"errors":[{"context":null,"message":"A detailed error message.","exceptionName":null}]}</ns3:code></ns3:pre></ns3:p></ns2:doc><ns2:doc><![CDATA[The currently authenticated user has insufficient permissions to view the pull
                                  request.]]></ns2:doc></ns2:representation><ns2:representation
                        status="204"><ns2:doc><![CDATA[The user is no longer watching the commit.]]></ns2:doc></ns2:representation><ns2:representation
                        element="errors" status="404"
                                mediaType="application/json"><ns2:doc><ns3:p
                xmlns:ns3="http://www.w3.org/1999/xhtml"><ns3:h6>Example</ns3:h6><ns3:pre><ns3:code>{"errors":[{"context":null,"message":"A detailed error message.","exceptionName":null}]}</ns3:code></ns3:pre></ns3:p></ns2:doc><ns2:doc><![CDATA[The specified project, repository or commit does not exist.]]></ns2:doc></ns2:representation></ns2:response></ns2:method><ns2:method
                id="watch"
                        name="POST"><ns2:doc><![CDATA[Adds the authenticated user as a watcher for the specified commit.
 <p>
 The authenticated user must have <strong>REPO_READ</strong> permission for the repository containing the commit
 to call this resource.]]></ns2:doc><ns2:response><ns2:representation
                        element="errors" status="401"
                                mediaType="application/json"><ns2:doc><ns3:p
                        xmlns:ns3="http://www.w3.org/1999/xhtml"><ns3:h6>Example</ns3:h6><ns3:pre><ns3:code>{"errors":[{"context":null,"message":"A detailed error message.","exceptionName":null}]}</ns3:code></ns3:pre></ns3:p></ns2:doc><ns2:doc><![CDATA[The currently authenticated user has insufficient permissions to view the pull
                                  request.]]></ns2:doc></ns2:representation><ns2:representation
                        status="204"><ns2:doc><![CDATA[The user is now watching the commit.]]></ns2:doc></ns2:representation><ns2:representation
                        element="errors" status="404"
                                mediaType="application/json"><ns2:doc><ns3:p
            xmlns:ns3="http://www.w3.org/1999/xhtml"><ns3:h6>Example</ns3:h6><ns3:pre><ns3:code>{"errors":[{"context":null,"message":"A detailed error message.","exceptionName":null}]}</ns3:code></ns3:pre></ns3:p></ns2:doc><ns2:doc><![CDATA[The specified project, repository or commit does not exist.]]></ns2:doc></ns2:representation></ns2:response></ns2:method></ns2:resource><ns2:resource
                path="api/1.0/projects/{projectKey}/repos/{repositorySlug}/permissions"><ns2:resource
                    path="users/none"><ns2:method
                    id="getUsersWithoutPermission"
                            name="GET"><ns2:doc><![CDATA[Retrieve a page of <i>licensed</i> users that have no granted permissions for the specified repository.
 <p>
 The authenticated user must have <strong>REPO_ADMIN</strong> permission for the specified repository or a higher
 project or global permission to call this resource.]]></ns2:doc><ns2:request><ns2:param
                            name="filter" style="query" type="xs:string"
                            xmlns:xs="http://www.w3.org/2001/XMLSchema"><ns2:doc><![CDATA[if specified only group names containing the supplied string will be returned]]></ns2:doc></ns2:param></ns2:request><ns2:response><ns2:representation
                            element="page" status="200"
                                    mediaType="application/json"><ns2:doc><ns3:p
                            xmlns:ns3="http://www.w3.org/1999/xhtml"><ns3:h6>Example</ns3:h6><ns3:pre><ns3:code>{"size":1,"limit":25,"isLastPage":true,"values":[{"name":"group-a","deletable":true}],"start":0}</ns3:code></ns3:pre></ns3:p></ns2:doc><ns2:doc><![CDATA[A page of users that have not been granted any permissions for the specified
                                  repository.]]></ns2:doc></ns2:representation><ns2:representation
                            element="errors" status="401"
                                    mediaType="application/json"><ns2:doc><ns3:p
                            xmlns:ns3="http://www.w3.org/1999/xhtml"><ns3:h6>Example</ns3:h6><ns3:pre><ns3:code>{"errors":[{"context":null,"message":"A detailed error message.","exceptionName":null}]}</ns3:code></ns3:pre></ns3:p></ns2:doc><ns2:doc><![CDATA[The currently authenticated user is not a repository administrator for the
                                  specified repository.]]></ns2:doc></ns2:representation><ns2:representation
                            element="errors" status="404"
                                    mediaType="application/json"><ns2:doc><ns3:p
                xmlns:ns3="http://www.w3.org/1999/xhtml"><ns3:h6>Example</ns3:h6><ns3:pre><ns3:code>{"errors":[{"context":null,"message":"A detailed error message.","exceptionName":null}]}</ns3:code></ns3:pre></ns3:p></ns2:doc><ns2:doc><![CDATA[The specified repository does not exist.]]></ns2:doc></ns2:representation></ns2:response></ns2:method></ns2:resource><ns2:resource
                    path="groups"><ns2:method id="setPermissionForGroup"
                            name="PUT"><ns2:doc><![CDATA[Promote or demote a group's permission level for the specified repository. Available repository permissions are:
 <ul>
     <li>REPO_READ</li>
     <li>REPO_WRITE</li>
     <li>REPO_ADMIN</li>
 </ul>
 See the <a href="https://confluence.atlassian.com/display/BitbucketServer/Using+repository+permissions">Bitbucket
 Server documentation</a> for a detailed explanation of what each permission entails.
 <p>
 The authenticated user must have <strong>REPO_ADMIN</strong> permission for the specified repository or a higher
 project or global permission to call this resource. In addition, a user may not demote a group's permission level if their
 own permission level would be reduced as a result.]]></ns2:doc><ns2:request><ns2:param
                            name="permission" style="query"
                            type="xs:string"
                            xmlns:xs="http://www.w3.org/2001/XMLSchema"><ns2:doc><![CDATA[the permission to grant]]></ns2:doc></ns2:param><ns2:param
                            name="name" style="query" type="xs:string"
                            xmlns:xs="http://www.w3.org/2001/XMLSchema"><ns2:doc><![CDATA[the names of the groups]]></ns2:doc></ns2:param></ns2:request><ns2:response><ns2:representation
                            element="errors" status="400"
                                    mediaType="application/json"><ns2:doc><ns3:p
                            xmlns:ns3="http://www.w3.org/1999/xhtml"><ns3:h6>Example</ns3:h6><ns3:pre><ns3:code>{"errors":[{"context":"field_a","message":"A detailed validation error message for field_a.","exceptionName":null},{"context":null,"message":"A detailed error message.","exceptionName":null}]}</ns3:code></ns3:pre></ns3:p></ns2:doc><ns2:doc><![CDATA[The request was malformed or the specified permission does not exist.]]></ns2:doc></ns2:representation><ns2:representation
                            element="errors" status="401"
                                    mediaType="application/json"><ns2:doc><ns3:p
                            xmlns:ns3="http://www.w3.org/1999/xhtml"><ns3:h6>Example</ns3:h6><ns3:pre><ns3:code>{"errors":[{"context":null,"message":"A detailed error message.","exceptionName":null}]}</ns3:code></ns3:pre></ns3:p></ns2:doc><ns2:doc><![CDATA[The currently authenticated user is not an administrator for the specified
                                  repository.]]></ns2:doc></ns2:representation><ns2:representation
                            status="204"><ns2:doc><![CDATA[The requested permission was granted.]]></ns2:doc></ns2:representation><ns2:representation
                            element="errors" status="403"
                                    mediaType="application/json"><ns2:doc><ns3:p
                            xmlns:ns3="http://www.w3.org/1999/xhtml"><ns3:h6>Example</ns3:h6><ns3:pre><ns3:code>{"errors":[{"context":null,"message":"A detailed error message.","exceptionName":null}]}</ns3:code></ns3:pre></ns3:p></ns2:doc><ns2:doc><![CDATA[The action was disallowed as it would reduce the currently authenticated user's
                                  permission level.]]></ns2:doc></ns2:representation><ns2:representation
                            element="errors" status="404"
                                    mediaType="application/json"><ns2:doc><ns3:p
                    xmlns:ns3="http://www.w3.org/1999/xhtml"><ns3:h6>Example</ns3:h6><ns3:pre><ns3:code>{"errors":[{"context":null,"message":"A detailed error message.","exceptionName":null}]}</ns3:code></ns3:pre></ns3:p></ns2:doc><ns2:doc><![CDATA[The specified repository or group does not exist.]]></ns2:doc></ns2:representation></ns2:response></ns2:method><ns2:method
                    id="getGroupsWithAnyPermission"
                            name="GET"><ns2:doc><![CDATA[Retrieve a page of groups that have been granted at least one permission for the specified repository.
 <p>
 The authenticated user must have <strong>REPO_ADMIN</strong> permission for the specified repository or a higher
 project or global permission to call this resource.]]></ns2:doc><ns2:request><ns2:param
                            name="filter" style="query" type="xs:string"
                            xmlns:xs="http://www.w3.org/2001/XMLSchema"><ns2:doc><![CDATA[if specified only group names containing the supplied string will be returned]]></ns2:doc></ns2:param></ns2:request><ns2:response><ns2:representation
                            element="page" status="200"
                                    mediaType="application/json"><ns2:doc><ns3:p
                            xmlns:ns3="http://www.w3.org/1999/xhtml"><ns3:h6>Example</ns3:h6><ns3:pre><ns3:code>{"size":1,"limit":25,"isLastPage":true,"values":[{"group":{"name":"group_a"},"permission":"ADMIN"}],"start":0}</ns3:code></ns3:pre></ns3:p></ns2:doc><ns2:doc><![CDATA[A page of groups and their highest permissions for the specified repository.]]></ns2:doc></ns2:representation><ns2:representation
                            element="errors" status="401"
                                    mediaType="application/json"><ns2:doc><ns3:p
                            xmlns:ns3="http://www.w3.org/1999/xhtml"><ns3:h6>Example</ns3:h6><ns3:pre><ns3:code>{"errors":[{"context":null,"message":"A detailed error message.","exceptionName":null}]}</ns3:code></ns3:pre></ns3:p></ns2:doc><ns2:doc><![CDATA[The currently authenticated user is not a repository administrator for the
                                  specified repository.]]></ns2:doc></ns2:representation><ns2:representation
                            element="errors" status="404"
                                    mediaType="application/json"><ns2:doc><ns3:p
                    xmlns:ns3="http://www.w3.org/1999/xhtml"><ns3:h6>Example</ns3:h6><ns3:pre><ns3:code>{"errors":[{"context":null,"message":"A detailed error message.","exceptionName":null}]}</ns3:code></ns3:pre></ns3:p></ns2:doc><ns2:doc><![CDATA[The specified repository does not exist.]]></ns2:doc></ns2:representation></ns2:response></ns2:method><ns2:method
                    id="revokePermissionsForGroup"
                            name="DELETE"><ns2:doc><![CDATA[Revoke all permissions for the specified repository for a group.
 <p>
 The authenticated user must have <strong>REPO_ADMIN</strong> permission for the specified repository or a higher
 project or global permission to call this resource.
 <p>
 In addition, a user may not revoke a group's permissions if it will reduce their own permission level.]]></ns2:doc><ns2:request><ns2:param
                            name="name" style="query" type="xs:string"
                            xmlns:xs="http://www.w3.org/2001/XMLSchema"><ns2:doc><![CDATA[the name of the group]]></ns2:doc></ns2:param></ns2:request><ns2:response><ns2:representation
                            element="errors" status="401"
